@model ViewmMODeElMASTER
@{
    ViewData["Title"] = "ListProduct";
    Layout = "~/Areas/merchantAccount/Views/Shared/_LayoutmerchantAr.cshtml";
}
<h2>ListProduct</h2>

<div class="container mt-5">
    <h1>Product List</h1>

    <form role="form" method="post" asp-controller="Home" asp-action="ListProduct">
        <div class="form-group">
            <label for="urlInput">Enter URL</label>
            <input type="text" id="urlInput" name="url" placeholder="Enter URL" class="form-control" maxlength="300">
        </div>
        <button type="submit" class="btn btn-primary btn-sm waves-effect waves-light">Submit</button>
    </form>

    @if (Model != null && Model.Listprodatt.Any())
    {
        <table class="table table-bordered mt-4">
            <thead>
                <tr>
                    <th>Product</th>
                    <th>Make</th>
                    <th>Model</th>
                    <th>UPC</th>
                    <th>Quantity</th>
                    <th>Retail Price</th>
                    <th>Total Retail Price</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var product in Model.Listprodatt)
                {
                    <tr>
                        <td>@product.ProductName</td>
                        <td>@product.Make</td>
                        <td>@product.Model</td>
                        <td>@product.UPC</td>
                        <td>@product.Quantity</td>
                        <td>@product.RetailPrice</td>
                        <td>@product.TotalRetailPrice</td>
                    </tr>
                }
            </tbody>
        </table>
    }
    }

