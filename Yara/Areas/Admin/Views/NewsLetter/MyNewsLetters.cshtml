@model ViewmMODeElMASTER
@{
    ViewData["Title"] = "MyNewsLetterGroups";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="col-xs-12">
    <div class="box-content">
        <ul class="list-inline">
            <li class="margin-bottom-10"><a href="#"><button type="button" class="btn btn-primary btn-rounded btn-bordered waves-effect waves-light">@ResourceWeb.LbMyNewsLetterGroups</button></a></li>/
            <li class="margin-bottom-10"><a asp-area="Admin" asp-controller="Home" asp-action="Index"><button type="button" class="btn btn-primary btn-rounded btn-bordered waves-effect waves-light">Admin Home</button></a></li>/
            <li class="margin-bottom-10"><a asp-area="" asp-controller="Home" asp-action="Index"><button type="button" class="btn btn-primary btn-rounded btn-bordered waves-effect waves-light">Home</button></a></li>
            <span style="color:royalblue"> <strong>@TempData["Saved successfullyl"]</strong></span>
            <span style="color:red"> <strong>@TempData["ErrorSave"]</strong></span>
        </ul>
        <!-- /.box-title -->
        <div class="dropdown js__drop_down">
            <a asp-area="Admin" asp-controller="NewsLetter" asp-action="AddNewsLetters"><button type="button" class="btn btn-violet btn-rounded btn-bordered waves-effect waves-light" style="margin-bottom:20px">Add</button></a>
        </div>
        <!-- /.dropdown js__dropdown -->
        <div id="example_wrapper" class="dataTables_wrapper form-inline dt-bootstrap">
            <div class="row">
                <div class="col-sm-12">
                    <div id="rootwizard-progress">
                        <div class="tab-header">
                            <div class="navbar">
                                <span style="color:royalblue"> <strong>@TempData["Saved successfully"]</strong></span>
                                <span style="color:red"> <strong>@TempData["ErrorSave"]</strong></span>
                                <div class="navbar-inner">
                                    <ul>
                                        <li><a href="#tab-progress-1" data-toggle="tab">@ResourceWeb.LbNewsLetter</a></li>

                                        <li><a href="#tab-progress-2" data-toggle="tab">@ResourceWeb.LbUserInformaition</a></li>

                                        <li><a href="#tab-progress-3" data-toggle="tab">@ResourceWeb.LbInformaition</a></li>
                                        @*  <li><a href="#tab-progress-4" data-toggle="tab">Forth</a></li>
                                        <li><a href="#tab-progress-5" data-toggle="tab">Fifth</a></li> *@
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="tab-content">
                            <div id="bar" class="progress">
                                <div class="progress-bar active progress-bar-info progress-bar-striped bar"></div>
                            </div>
                            <div class="tab-pane" id="tab-progress-1">
                                <div class='table-responsive'>
                                    <div style="overflow-x:auto;">
                                        <table id="example" class="table table-striped table-bordered table-hover display dataTable responsive" style="width: 100%;" role="grid" aria-describedby="example_info">

                                            <thead>
                                                <tr role="row">
                                                    <th class="sorting_asc" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Name: activate to sort column descending">@ResourceWeb.LbId</th>
                                                    <th class="sorting" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 64px;">@ResourceWeb.LbGroupName</th>
                                                    <th class="sorting" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 64px;">@ResourceWeb.LbIdTemplate</th>
                                                    <th class="sorting" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 64px;">@ResourceWeb.LbTemplateName</th>
                                                    <th class="sorting" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 64px;">@ResourceWeb.LbContaint</th>
                                                    <th class="sorting" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 64px;">@ResourceWeb.LbGroupName</th>
                                            <tfoot>
                                                <tr>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbId</th>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbTitle</th>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbIdTemplate</th>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbTemplateName</th>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbContaint</th>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbGroupName</th>


                                            </tfoot>
                                            <tbody>
                                                        @foreach (var item in Model.ListNewsLetter)
                                                        {
                                                    <tr role="row" class="odd">
                                                        <td class="truncate-50">@item.IdNewsletter</td>
                                                        <td class="truncate-50">@item.Title</td>

                                                        <td class="truncate-50">@item.IdTemplate</td>
                                                        <td class="truncate-50">@item.TemplateName</td>
                                                        <td class="truncate-50">@item.Containt</td>
                                                        <td class="truncate-50">@item.GroupName</td>
                                                    </tr>
                                                        }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>

                            <div class="tab-pane" id="tab-progress-2">
                                <div class='table-responsive'>
                                    <div style="overflow-x:auto;">
                                        <table id="example" class="table table-striped table-bordered table-hover display dataTable responsive" style="width: 100%;" role="grid" aria-describedby="example_info">

                                            <thead>
                                                <tr role="row">
                                                    <th class="sorting_asc" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Name: activate to sort column descending">@ResourceWeb.LbTitle</th>
                                                    <th class="sorting" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 64px;">@ResourceWeb.LbUserName</th>
                                                    <th class="sorting" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 64px;">@ResourceWeb.LbImageUser</th>
                                                    <th class="sorting" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 64px;">@ResourceWeb.LbCurrentEmail</th>
                                                    <th class="sorting" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 64px;">@ResourceWeb.LbConstEmail</th>
                                                    <th class="sorting" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 64px;">@ResourceWeb.LbPhoneNumber</th>
                                            <tfoot>
                                                <tr>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbTitle</th>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbUserName</th>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbImageUser</th>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbCurrentEmail</th>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbConstEmail</th>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbPhoneNumber</th>

                                            </tfoot>
                                            <tbody>
                                                        @foreach (var item in Model.ListNewsLetter)
                                                        {
                                                    <tr role="row" class="odd">
                                                        <td class="truncate-50">@item.Title</td>
                                                        <td class="truncate-50">@item.UserName</td>
                                                        <td class="truncate-50">@item.ImageUser</td>
                                                        <td class="truncate-50">@item.CurrentEmail</td>
                                                        <td class="truncate-50">@item.ConstEmail</td>
                                                        <td class="truncate-50">@item.PhoneNumber</td>
                                                    </tr>
                                                        }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>

                            <div class="tab-pane" id="tab-progress-3">
                                <div class='table-responsive'>
                                    <div style="overflow-x:auto;">
                                        <table id="example2" class="table table-striped table-bordered display dataTable table-responsive" style="width: 100%;" role="grid" aria-describedby="example_info">
                                            <thead>
                                                <tr role="row">
                                                    <th class="sorting_asc" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Name: activate to sort column descending">@ResourceWeb.LbId</th>
                                                    <th class="sorting" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 64px;">@ResourceWeb.LbDataEntry</th>
                                                    <th class="sorting" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 64px;">@ResourceWeb.LbDateTimeEntry</th>
                                                    <th class="sorting" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 64px;">@ResourceWeb.lbCurrentState</th>
                                                    <th class="sorting" tabindex="0" aria-controls="example" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending">@ResourceWeb.LbOpration</th>
                                            <tfoot>
                                                <tr>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbId</th>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbDataEntry</th>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbDateTimeEntry</th>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.lbCurrentState</th>
                                                    <th rowspan="1" colspan="1">@ResourceWeb.LbOpration</th>
                                            </tfoot>
                                            <tbody>
                                                        @foreach (var item in Model.ListNewsLetter)
                                                        {
                                                    <tr role="row" class="odd">
                                                        <td >@item.Title</td>

                                                        <td>@item.DataEntry</td>
                                                        <td>@item.DateTimeEntry</td>
                                                        <td>
                                                        <div class="switch primary"><input type="checkbox" checked="@item.CurrentState" IdTypesOfMessage="switch-2" disabled><label for="switch-2">@item.CurrentState</label></div>
                                                        </td>
                                                        <td>
                                                            <ul class="list-inline">
                                                                <li class="margin-bottom-10"><a asp-area="Admin" asp-controller="NewsLetter" asp-action="AddNewsLetter" asp-route-IdNewsletter="@item.IdNewsletter"><button type="button" class="btn btn-primary btn-circle btn-lg waves-effect waves-light"><i class="ico fa fa-edit"></i></button></a></li>|
                                                                <li class="margin-bottom-10"><a asp-area="Admin" asp-controller="NewsLetter" asp-action="DeleteData" asp-route-IdNewsletter="@item.IdNewsletter"><button type="button" class="btn btn-danger btn-circle btn-lg waves-effect waves-light"><i class="ico fa fa-remove"></i></button></a></li>
                                                            </ul>
                                                        </td>
                                                    </tr>
                                                        }
                                            </tbody>
                                        </table>

                                    </div>
                                </div>
                            </div>
                            <ul class="pager wizard">
                                <li class="previous"><a href="javascript:void(0)">Previous</a></li>
                                <li class="next"><a href="javascript:void(0)">Next</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-5">
                <div class="dataTables_info" id="example_info" role="status" aria-live="polite">Showing 1 to 4 of 4 entries (filtered from 57 total entries)</div>
            </div><div class="col-sm-7"><div class="dataTables_paginate paging_simple_numbers" id="example_paginate"><ul class="pagination"><li class="paginate_button previous disabled" id="example_previous"><a href="#" aria-controls="example" data-dt-idx="0" tabindex="0">Previous</a></li><li class="paginate_button active"><a href="#" aria-controls="example" data-dt-idx="1" tabindex="0">1</a></li><li class="paginate_button next disabled" id="example_next"><a href="#" aria-controls="example" data-dt-idx="2" tabindex="0">Next</a></li></ul></div></div>
        </div>
    </div>
</div>
